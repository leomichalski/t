<%
if respond_to?(:current_component) && current_component && can_be_managed?(current_component)
  edit_link(
    manage_component_path(current_component),
    :read,
    :component,
    component: current_component,
    current_participatory_space: current_participatory_space
  )
end
%>

<link rel="stylesheet" href="https://cdngovbr-ds.estaleiro.serpro.gov.br/design-system/fonts/rawline/css/rawline.css"/>
<link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Raleway:300,400,500,600,700,800,900&amp;display=swap"/>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.2/css/all.min.css" />

<div class="template-base">
  <!-- data-sticky="data-sticky" -->
  <header class="br-header mb-4" id="br-header">
    <div class="br-container-lg">
      <div class="header-top">
        <div class="header-logo">
          <a href="https://www.gov.br/pt-br">
            <img src="data:image/png;base64," alt="gov.br logo"/>
          </a>
          <!--<div class="header-sign">Assinatura</div>-->
        </div>
        <div class="header-actions">
          <!--
          <div class="header-links dropdown">
            <button class="br-button circle small" type="button" data-toggle="dropdown" aria-label="Abrir Acesso Rápido"><i class="fas fa-ellipsis-v" aria-hidden="true"></i>
            </button>
            <div class="br-list">
              <div class="header">
                <div class="title">Acesso Rápido</div>
              </div><a class="br-item" href="javascript:void(0)">Link de acesso 1</a><a class="br-item" href="javascript:void(0)">Link de acesso 2</a><a class="br-item" href="javascript:void(0)">Link de acesso 3</a><a class="br-item" href="javascript:void(0)">Link de acesso 4</a>
            </div>
            <span class="br-divider vertical mx-half mx-sm-1"></span>
          </div>
          -->
          <div class="header-functions dropdown">
            <button class="br-button circle small" type="button" data-toggle="dropdown" aria-label="Abrir Funcionalidades do Sistema"><i class="fas fa-th" aria-hidden="true"></i>
            </button>
            <div class="br-list">
              <div class="header">
                <div class="title">Funcionalidades do Sistema</div>
              </div>
              <%
=begin%>
 <% if current_user %>
                <%= link_to decidim.notifications_path, class: "#{current_user.notifications.any? ? "is-active" : ""}" do %>
                  <div class="br-item">
                    <button class="br-button circle small" type="button" aria-label="Notificações">
                        <i class="fas fa-bell" aria-hidden="true"></i>
                      <span class="text">Notificações</span>
                    </button>
                  </div>
                <% end %>
                <%= link_to decidim.conversations_path, class: "#{current_user.unread_conversations.any? ? "is-active" : ""}" do %>
                  <div class="br-item">
                    <button class="br-button circle small" type="button" aria-label="Conversas">
                        <i class="fas fa-message" aria-hidden="true"></i>
                      <span class="text">Conversas</span>
                    </button>
                  </div>
                <% end %>
              <% end %>
<%
=end%>
              <div href="#" data-open="dc-modal" aria-controls="dc-modal" aria-haspopup="dialog" tabindex="0">
                <div class="br-item">
                  <button class="br-button circle small" type="button" aria-label="Redefinir Cookies">
                    <i class="fas fa-cookie-bite" aria-hidden="true"></i>
                    <span class="text">Redefinir Cookies</span>
                  </button>
                </div>
              </div>

              <div class="br-item">
                <button class="br-button circle small" id="contrast-button" type="button" aria-label="Mudar para o modo de alto contraste">
                  <i class="fas fa-adjust" aria-hidden="true"></i>
                  <span class="text">Mudar para o modo de alto contraste</span>
                </button>
              </div> 
            </div>
          </div>
          <div class="header-search-trigger">
            <button class="br-button circle" type="button" aria-label="Abrir Busca"
              data-toggle="search" data-target=".header-search">
              <i class="fas fa-search" aria-hidden="true"></i>
            </button>
          </div>
          <% if current_user %>
            <div>
              <button class="br-sign-in" type="button" id="avatar-dropdown-trigger"
                data-toggle="dropdown" data-target="avatar-menu" aria-label="Avatar com dropdown">
                <span class="br-avatar" title=""></span>
                <span class="ml-2 text-gray-80 text-weight-regular br-dropdown-text">
                  Olá, <span class="text-weight-semi-bold current-user-dropdown"><%= current_user.name %></span>
                </span>
                <i class="fas fa-caret-down" aria-hidden="true"></i>
              </button>
              <div class="br-list-dropdown" id="avatar-menu" hidden="hidden">
                <%= render partial: "layouts/decidim/user_menu" %>
              </div>
            </div>
          <% else %>
            <div class="header-login">
              <div class="header-sign-in">
                <%= link_to decidim.new_user_session_path, class: "sign-in-button" do %>
                  <button class="br-sign-in small" type="button" data-trigger="login">
                    <i class="fas fa-user" aria-hidden="true"></i><span class="d-sm-inline">Entrar</span>
                  </button>
                <% end %>
              </div>
            </div>
          <% end %>
        </div>
      </div>
      <div class="header-bottom">
        <div class="header-menu">
          <div class="header-menu-trigger" id="header-navigation">
            <button class="br-button small circle" type="button" aria-label="Menu"
              data-toggle="menu" data-target="#main-navigation" id="navigation">
              <i class="fas fa-bars" aria-hidden="true"></i>
            </button>
          </div>
          <div class="header-info">
            <a href="/"><div class="header-title">Brasil Participativo</div></a>
            <!--<div class="header-subtitle">Subtítulo do Header</div>-->
          </div>
        </div>
        <form action="/search" accept-charset="UTF-8" method="get">
          <div class="header-search" id="main-searchbox">
            <div class="br-input has-icon">
              <label for="searchbox">Texto da pesquisa</label>
              <input name="term" id="searchbox" type="text" placeholder="O que você procura?" />
              <button class="br-button circle small" type="submit" aria-label="Pesquisar">
                <i class="fas fa-search" aria-hidden="true"></i>
              </button>
            </div>
            <button class="br-button circle search-close ml-1"
              type="button" aria-label="Fechar Busca" data-dismiss="search">
              <i class="fas fa-times" aria-hidden="true"></i>
            </button>
          </div>
        </form>
      </div>
    </div>
  </header>
  <%= display_flash_messages %>
  <main id="content">
    <div class="br-container-lg d-flex">
      <div class="br-row">
        <div class="br-menu" id="main-navigation">
          <div class="menu-container">
            <div class="menu-panel">
              <div class="menu-header">
                <div class="menu-title">
                  <img class="mobile-logo" src="data:image/png;base64," alt="gov.br logo mobile"/>
                  <a href="/" class="bp-link-menu">Brasil Participativo</a>
                </div>
                <div class="menu-close">
                  <button class="br-button circle" type="button" aria-label="Fechar o menu" data-dismiss="menu">
                    <i class="fas fa-times" aria-hidden="true"></i>
                  </button>
                </div>
              </div>
              <%= render_menu_links(current_organization) %>
            </div>
            <div class="menu-scrim" data-dismiss="menu" tabindex="0"></div>
            </div>
          </div>
        </div>
      </div>

      <%= yield %>

    </div>
  </main>
  <div class="footer" role="contentinfo">
    <%= render partial: "layouts/decidim/main_footer" %>
    <%= render partial: "layouts/decidim/mini_footer" %>
  </div>
</div>